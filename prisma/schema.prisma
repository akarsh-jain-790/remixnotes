generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model notes {
  id          String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  date        DateTime @db.Date
  description String
  tag         String
  title       String
}
